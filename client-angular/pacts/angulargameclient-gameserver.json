{
  "consumer": {
    "name": "AngularGameClient"
  },
  "provider": {
    "name": "GameServer"
  },
  "interactions": [
    {
      "description": "start new game",
      "request": {
        "method": "POST",
        "path": "/games/"
      },
      "response": {
        "status": 201,
        "headers": {
        },
        "body": {
          "id": "1234"
        },
        "matchingRules": {
          "$.body.id": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "guessed letter a",
      "providerState": "a game with id 1234-5678 is running",
      "request": {
        "method": "POST",
        "path": "/games/1234-5678/guesses",
        "body": {
          "guess": "a"
        }
      },
      "response": {
        "status": 201,
        "headers": {
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}